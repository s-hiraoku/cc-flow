{
  "name": "@hiraoku/cc-flow-cli",
  "version": "0.0.9",
  "description": "Interactive TUI for creating Claude Code workflows",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist/",
    "bin/",
    "scripts/",
    "templates/",
    "README.md",
    "LICENSE"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./cli": {
      "import": "./dist/cli/main.js",
      "types": "./dist/cli/main.d.ts"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "cc-flow": "./bin/cc-flow.js"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "compile": "tsc",
    "clean": "rm -rf dist",
    "dev": "tsx src/cli/main.ts",
    "start": "node dist/cli/main.js",
    "prepublishOnly": "npm run build",
    "postbuild": "npm run fix-bin",
    "fix-bin": "node scripts/fix-bin.js",
    "lint": "tsc --noEmit",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:compile": "tsc src/test-compile.ts --outDir dist-test --module ESNext --target ES2022 --moduleResolution Node",
    "test:build": "node scripts/test-build.js",
    "validate": "npm run type-check && npm run build",
    "demo-reorder": "tsx src/demo-reorder.ts"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "claude-code",
    "workflow",
    "tui",
    "cli",
    "automation",
    "terminal-ui"
  ],
  "author": "CC-Flow Team",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/s-hiraoku/cc-flow",
    "directory": "cc-flow-cli"
  },
  "homepage": "https://github.com/s-hiraoku/cc-flow/tree/main/cc-flow-cli#readme",
  "bugs": {
    "url": "https://github.com/s-hiraoku/cc-flow/issues"
  },
  "dependencies": {
    "@inquirer/checkbox": "^2.0.0",
    "@inquirer/input": "^2.0.0",
    "@inquirer/prompts": "^5.0.0",
    "@inquirer/select": "^2.0.0",
    "boxen": "^7.0.0",
    "chalk": "^5.3.0",
    "figlet": "^1.7.0"
  },
  "devDependencies": {
    "@types/figlet": "^1.5.8",
    "@types/node": "^22.0.0",
    "tsx": "^4.19.0",
    "typescript": "~5.6.0",
    "vitest": "^2.0.5",
    "@vitest/coverage-v8": "^2.0.5"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/s-hiraoku/cc-flow"
  },
  "security": {
    "contacts": [
      "security@cc-flow.dev"
    ],
    "policy": "https://github.com/s-hiraoku/cc-flow/security/policy"
  }
}
