{
  "name": "@hiraoku/cc-flow-cli",
  "version": "0.0.11",
  "description": "Interactive TUI for creating Claude Code workflows",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist/",
    "bin/",
    "scripts/",
    "templates/",
    "README.md",
    "LICENSE"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./ink": {
      "import": "./dist/ink/main.js",
      "types": "./dist/ink/main.d.ts"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "cc-flow": "./bin/cc-flow.js"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "compile": "tsc",
    "clean": "rm -rf dist",
    "dev": "tsx src/ink/main.tsx",
    "start": "node dist/ink/main.js",
    "prepublishOnly": "npm run build",
    "postbuild": "npm run fix-bin",
    "fix-bin": "node scripts/fix-bin.js",
    "lint": "tsc --noEmit",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:compile": "tsc src/test-compile.ts --outDir dist-test --module ESNext --target ES2022 --moduleResolution Node",
    "test:build": "node scripts/test-build.js",
    "validate": "npm run type-check && npm run build"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "claude-code",
    "workflow",
    "tui",
    "cli",
    "automation",
    "terminal-ui"
  ],
  "author": "CC-Flow Team",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/s-hiraoku/cc-flow",
    "directory": "cc-flow-cli"
  },
  "homepage": "https://github.com/s-hiraoku/cc-flow/tree/main/cc-flow-cli#readme",
  "bugs": {
    "url": "https://github.com/s-hiraoku/cc-flow/issues"
  },
  "dependencies": {
    "@types/react": "^19.1.13",
    "date-and-time": "^4.0.5",
    "ink": "^6.3.0",
    "ink-big-text": "^2.0.0",
    "ink-gradient": "^3.0.0",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "react": "^19.1.1",
    "string-width": "^7.2.0",
    "strip-ansi": "^7.1.2",
    "wrap-ansi": "^9.0.2"
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "ink-testing-library": "^4.0.0",
    "tsx": "^4.19.0",
    "typescript": "~5.6.0",
    "vitest": "^3.2.4"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/s-hiraoku/cc-flow"
  },
  "security": {
    "contacts": [
      "security@cc-flow.dev"
    ],
    "policy": "https://github.com/s-hiraoku/cc-flow/security/policy"
  }
}
